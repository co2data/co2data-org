{
  "name": "co2data",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "lint": "biome check app domain adapter lib",
    "build:worker": "opennextjs-cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview": "npm run build:worker && npm run dev:worker",
    "deploy": "npm run build:worker && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "ci": "vitest run && next build",
    "analyze-bundle-size": "ANALYZE=true next build",
    "db-migrate": "drizzle-kit generate",
    "db-push-local": "wrangler d1 migrations apply prod-d1-co2data",
    "db-push-remote": "wrangler d1 migrations apply prod-d1-co2data --remote",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "lefthook install"
  },
  "dependencies": {
    "@effect/opentelemetry": "^0.44.2",
    "@microlabs/otel-cf-workers": "1.0.0-rc.49",
    "@next/bundle-analyzer": "15.1.7",
    "@opentelemetry/exporter-trace-otlp-http": "0.57.2",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/sdk-trace-web": "^1.30.1",
    "@planetscale/database": "^1.19.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "caniuse-lite": "^1.0.30001700",
    "class-variance-authority": "^0.7.1",
    "convert": "^5.8.0",
    "drizzle-orm": "^0.39.3",
    "effect": "3.13.2",
    "geist": "^1.3.1",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.475.0",
    "marked": "^15.0.7",
    "mime": "^4.0.6",
    "nanoid": "^5.1.0",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@opennextjs/cloudflare": "^0.5.3",
    "@playwright/test": "^1.50.1",
    "@simplewebauthn/types": "^12.0.0",
    "@tailwindcss/postcss": "^4.0.8",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "drizzle-kit": "0.30.4",
    "happy-dom": "^17.1.1",
    "jsdom": "^26.0.0",
    "lefthook": "^1.10.10",
    "lefthook-linux-x64": "^1.10.10",
    "mysql2": "^3.12.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.8",
    "tailwindcss-animate": "^1.0.7",
    "testtriple": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "5.7.3",
    "vitest": "^3.0.6",
    "wrangler": "^3.109.2"
  },
  "packageManager": "pnpm@8.10.5"
}
